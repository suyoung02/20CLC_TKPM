<div class="card" style="margin-top: 100px;">
    <div class="card-header d-flex justify-content-between">
        <h4>User</h4>
    </div>
    <div class="card-body">
        <table class="table table-hover">
            <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Address</th>
                <th>Phone number</th>
            </tr>
            </thead>
            <tbody>
            {{#each this.users}}
                <tr>
                    <td>{{Name}}</td>
                    <td>{{Gmail}}</td>
                    <td>{{Address}}</td>
                    <td>{{Phone}}</td>
            {{/each}}
            </tr>
            </tbody>
        </table>
    </div>

</div>